super + w
        $BROWSER

super + v
        $TERMINAL -e vim

super + d
        dmenu_run

Print
        maim -u -i $(xdotool getactivewindow) | xclip -selection clipboard -t image/png

super + S
	dbeaver

ctrl + Print
        maim -s | xclip -selection clipboard -t image/png

super + Print
        $HOME/.local/bin/screencast.sh

super + p
        mpv $(xclip -o)

super + Return
        $TERMINAL

super + s
        center-term.sh

super + {equal, minus, BackSpace}
    	volume.sh {up, down, toggle}

super + m
        $HOME/.local/bin/mount.sh

ctrl + shift 1
	connect_soundcore.sh

ctrl + shift 2
	connect_huawei.sh

super + shift + BackSpace
        $HOME/.local/bin/exit.sh

super + a
        libreoffice

super + n
        $TERMINAL -e  nnn -Rr

super + z
        sleep.sh

super + r
        rawtherapee

super + c
        code

super + o
        mypaint

alt + w
        connect_wifi.sh

alt + shift + w
        disconnect_wifi.sh

super + g
        dbus-launch --exit-with-session flatpak run org.geogebra.GeoGebra

